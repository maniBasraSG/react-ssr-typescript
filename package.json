{
  "name": "react-final-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec node build/bundle.js",
    "dev:build-server": "webpack --config webpack/dev/webpack.dev.server.js --watch",
    "dev:build-client": "webpack --config  webpack/dev/webpack.dev.client.js --watch --no-cache",
    "build": "npm-run-all --parallel deploy:*",
    "deploy:prod-server": "webpack --config webpack/prod/webpack.prod.server.js",
    "deploy:prod-client": "webpack --config webpack/prod/webpack.prod.client.js",
    "format": "prettier --write --ignore-path .gitignore \"**/*.+(js|json|css|html|md)\"",
    "lint": "eslint src/client --fix --color  --ignore-path .gitignore ",
    "pre-commit-lint": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@loadable/component": "^5.15.0",
    "@loadable/server": "^5.15.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.3",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.25",
    "axios": "^0.27.2",
    "clean-webpack-plugin": "^4.0.0",
    "compression": "^1.7.4",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "express": "^4.18.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.1.0",
    "node-sass": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-loading-skeleton": "^3.1.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "styled-components": "^5.3.0",
    "typescript": "^4.3.2",
    "web-vitals": "^1.0.1",
    "webpack-dev-middleware": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.18.9",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.15.0",
    "@types/loadable__component": "^5.13.3",
    "@types/loadable__server": "^5.12.5",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-middleware": "^4.3.0",
    "@types/webpack-hot-middleware": "^2.25.4",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "css-loader": "^6.4.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^6.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.7.1",
    "redux-devtools-extension": "^2.13.9",
    "sass-loader": "^12.2.0",
    "style-loader": "^3.3.0",
    "terser-webpack-plugin": "^5.1.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^3.0.0"
  }
}
